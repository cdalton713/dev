{
  "name": "@cdalton713/eslint",
  "version": "0.0.1",
  "author": "Chrstian Dalton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cdalton713/dev.git",
    "directory": "packages/eslint"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "*.js",
    "dist/**/*",
    "lib/**/*",
    "src/**/*"
  ],
  "packemon": {
    "format": "lib",
    "platform": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "eslint-config-async": "^2.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-turbo": "^2.0.6",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "git+https://github.com/cdalton713/eslint-plugin-import.git#main",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-log-filenames": "^1.0.6",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  }
}
